{
    "name": "swr-simple-fetcher",
    "version": "1.0.0",
    "author": "Daniel Westbrook <westy12dan@gmail.com>",
    "description": "A simple fetcher for swr",
    "repository": {
        "type": "git",
        "url": "https://github.com/westbrookdaniel/swr-simple-fetcher.git"
    },
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --external react",
        "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
        "pretty": "prettier --write .",
        "lint": "eslint src**/*.ts* --fix",
        "clean": "rm -rf node_modules && rm -rf dist && rm -rf coverage",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "swr": "^1.3.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.10",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "c8": "^7.11.3",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "jsdom": "^19.0.0",
        "prettier": "^2.6.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "tsup": "^6.0.1",
        "typescript": "^4.7.2",
        "vitest": "^0.13.1"
    },
    "sideEffects": false,
    "keywords": [
        "react",
        "swr",
        "fetcher",
        "query",
        "simple"
    ]
}
